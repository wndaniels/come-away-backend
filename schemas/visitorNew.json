{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "fullName": {
      "type": "string",
      "minLength": 1
    },
    "note": {
      "type": "string",
      "minLength": 0
    },
    "startTime": {
      "type": "string",
      "minimum": 1
    },
    "endTime": {
      "type": "string",
      "minimum": 1
    },
    "calendarId":{
      "type": "integer",
      "minimum": 1
    }
  },
  "additionalProperties": false,
  "required": [
    "fullName",
    "note",
    "startTime",
    "endTime",
    "calendarId"
  ]
}
